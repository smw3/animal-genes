<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
    <name>Animal Genes</name>
    <description>This mod adds genes to sapient animals, allowing them to be extracted. In addition, generates a number of genes based on animal properties automatically to represent their abilities with.</description>
    <author>Ingendum</author>
    <supportedVersions>
        <li>1.6</li>
    </supportedVersions>
    <packageId>ingendum.animalgenes</packageId>
	<modDependencies>
		<li>
			<packageId>brrainz.harmony</packageId>
			<displayName>Harmony</displayName>
			<steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
		</li>
		<li>
			<packageId>RedMattis.SapientAnimals</packageId>
			<displayName>Big and Small - Sapient Animals</displayName>
			<steamWorkshopUrl>steam://url/CommunityFilePage/3505241400</steamWorkshopUrl>
		</li>
	</modDependencies>
	<loadAfter>
		<li>brrainz.harmony</li>
		<li>ludeon.rimworld</li>
		<li>ludeon.rimworld.royalty</li>
		<li>ludeon.rimworld.ideology</li>
		<li>ludeon.rimworld.biotech</li>
		<li>ludeon.rimworld.anomaly</li>
		<li>RedMattis.Genes</li>
		<li>RedMattis.SapientAnimals</li>
	</loadAfter>
</ModMetaData>